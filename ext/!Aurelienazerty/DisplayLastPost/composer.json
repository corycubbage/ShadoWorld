{
    "name": "Aurelienazerty/DisplayLastPost",
    "type": "phpbb-extension",
    "description": "Display the lastest post of the previous page on the top of viewtopic.",
    "homepage": "http://www.team-azerty.com",
    "version": "1.1.4",
    "time": "2015-10-19",
    "keywords": [
        "phpbb",
        "extension",
        "post"
    ],
    "license": "GPL-2.0",
    "authors": [
        {
            "name": "Aurelienazerty",
            "homepage": "http://www.team-azerty.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.3"
    },
    "extra": {
        "display-name": "Display Last Post",
        "soft-require": {
            "phpbb/phpbb": "3.1.*"
        },
        "version-check": {
            "host": "www.phpbb.com",
            "directory": "/customise/db/extension/displaylastpost",
            "filename": "version_check"
        }
    }
}